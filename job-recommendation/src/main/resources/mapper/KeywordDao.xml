<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jwang261.dao.KeywordDao">
    <select id="selectKeywordByItemId" parameterType="String" resultType="String">
        SELECT keyword from keywords WHERE item_id = #{itemId};
    </select>


</mapper>